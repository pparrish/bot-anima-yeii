{
  "name": "anima-yeii-bot",
  "version": "1.8.0-pre",
  "engines": {
    "node": "12.x"
  },
  "description": "A discord bot to manage Anima:beyond fantasy rolegame.",
  "main": "dist/index.js",
  "author": "Parrish",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec babel-node src/bot-service.js",
    "dev:exel": "nodemon --exec babel-node src/exel.js",
    "start:bot": "node dist/bot-service.js",
    "start:exel": "node dist/exel.js",
    "test": "npm run test:int  ",
    "test:noint": "jest --detectOpenHandles --modulePathIgnorePatterns=.int.",
    "test:watch": "jest --watch",
    "test:int": "jest --detectOpenHandles --passWithNoTests",
    "lint": "lint:eslint .",
    "fix": "eslint --fix",
    "lint:eslint": "eslint",
    "doc": "documentation build src/** -f md -o documentation.md",
    "build": "babel src --out-dir dist --source-maps --copy-files"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged -c ./lint-staged.config.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "body-parser": "^1.19.0",
    "bull": "^3.12.1",
    "discord.js": "^11.5.1",
    "dotenv": "^8.0.0",
    "expr-eval": "^1.2.2",
    "express": "^4.17.1",
    "firebase-admin": "^8.8.0",
    "graphql-request": "^1.8.2",
    "i18n": "^0.8.3",
    "node-fetch": "^2.6.1",
    "nodemon": "^1.19.1",
    "table": "^5.4.6",
    "throng": "^4.0.0",
    "xlsx": "^0.15.2"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.13.0",
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "core-js": "^3.6.4",
    "documentation": "^12.1.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.1",
    "prettier": "^1.19.1",
    "regenerator-runtime": "^0.13.3"
  }
}
